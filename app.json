{
  "expo": {
    "name": "plateCalculator-React_Native",
    "slug": "plateCalculator-React_Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/plateCalcIcon.png",
    "splash": {
      "image": "./assets/plateCalcIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An app that will calculate what plates you need for your barbell at a given weight",
    "githubUrl": "https://github.com/AlexMNet/plateCalculator-React_Native"
  }
}
